#:import MapView kivy_garden.mapview
#:include common_object

<ServicesMapView@MapView>:
    lat: 34.79
    lon: 48.51
    zoom: 10
    on_zoom:
        self.zoom = 5 if self.zoom < 5 else self.zoom
    on_lat:
        self.start_getting_markers_in_fov()
    on_lon:
        self.start_getting_markers_in_fov()
